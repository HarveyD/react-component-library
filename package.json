{
  "name": "react-component-library",
  "version": "1.0.0",
  "main": "build/index.js",
  "module": "build/index.esm.js",
  "files": [
    "build"
  ],
  "licence": "MIT",
  "description": "A skeleton to create your own React component library using Rollup, Sass Modules, TypeScript, Google Typescript Style, React Cosmos and Jest",
  "scripts": {
    "cosmos": "cosmos",
    "cosmos:export": "cosmos-export",
    "build": "yarn clean && rollup --config config/rollup.config.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "lint": "run-p lint:**",
    "lint:javascript": "gts fix",
    "lint:others": "prettier --write --loglevel warn '**/*.{json,md,yml}'",
    "lint:styles": "stylelint --fix 'src/**/*.{css,scss}'",
    "prepare": "yarn compile",
    "pretest": "yarn compile",
    "posttest": "yarn check"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/barb-components/react-component-library.git"
  },
  "keywords": [
    "Google Typescript Style",
    "React",
    "Component",
    "Library",
    "Rollup",
    "Typescript",
    "Sass",
    "Sass Modules",
    "Cosmos"
  ],
  "author": "Andy Weir <andy@ocean-internet.co.uk>",
  "license": "MIT",
  "contributors": [
    "Harvey Delaney"
  ],
  "homepage": "https://github.com/barb-components/react-component-library#readme",
  "peerDependencies": {
    "react": "^16.13.1",
    "react-dom": "^16.13.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.2",
    "@babel/helper-create-regexp-features-plugin": "^7.10.1",
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.0.1",
    "@testing-library/jest-dom": "^5.9.0",
    "@testing-library/react": "^10.2.1",
    "@types/jest": "^25.2.3",
    "@types/node": "^14.0.12",
    "@types/react": "^16.9.35",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-preset-react-app": "^9.1.2",
    "confusing-browser-globals": "^1.0.9",
    "css-loader": "^3.5.3",
    "eslint": "^6.8.0",
    "eslint-plugin-flowtype": "^4.7.0",
    "eslint-plugin-import": "^2.21.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.4",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "gts": "^2.0.2",
    "html-webpack-plugin": "^4.3.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.0.1",
    "normalize.css": "^8.0.1",
    "npm-run-all": "^4.1.5",
    "pkg-dir": "^4.2.0",
    "pre-commit": "^1.2.2",
    "react": "^16.13.1",
    "react-cosmos": "^5.3.0",
    "react-dom": "^16.13.1",
    "rollup": "^2.15.0",
    "rollup-plugin-copy": "^3.3.0",
