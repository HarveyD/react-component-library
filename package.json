{
  "name": "practice-react-from-scratch-component-library",
  "version": "2.0.1",
  "description": "A collection of from-scratch simple UI components, intended for practicing React/CSS for interviews",
  "main": "build/index.js",
  "module": "build/index.esm.js",
  "files": [
    "build"
  ],
  "types": "build/index.d.ts",
  "scripts": {
    "build": "rollup -c",
    "test": "npm run lint && npm run prettier && jest",
    "test:fix": "npm run lint:fix && npm run prettier:fix && jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier -c .",
    "prettier:fix": "prettier -w .",
    "storybook": "start-storybook -p 6006",
    "storybook:export": "build-storybook",
    "generate": "node ./util/create-component",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/haggs/practice-react-from-scratch-component-library.git"
  },
  "author": "Dan Haggerty",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/haggs/practice-react-from-scratch-component-library/issues"
  },
  "homepage": "https://github.com/haggs/practice-react-from-scratch-component-library#readme",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@rollup/plugin-commonjs": "^17.1.0",
    "@rollup/plugin-node-resolve": "^11.2.1",
    "@storybook/addon-essentials": "^6.5.13",
    "@storybook/addon-postcss": "^2.0.0",
    "@storybook/react": "^6.5.13",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@types/jest": "^24.9.1",
    "@types/react": "^16.14.14",
    "@types/react-dom": "^16.9.14",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "@typescript-eslint/parser": "^5.48.1",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.2.2",
    "babel-preset-react-app": "^10.0.0",
    "eslint": "^8.31.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "rollup": "^2.56.3",
    "rollup-plugin-copy": "^3.4.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.5.6",
    "typescript": "^4.9.4"
  }
}
